@model IEnumerable<MinusBugs.Institute.Web.FrontEnd.Models.Staff>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>ViewStaffDetails</h2>
        <ol class="breadcrumb">
            <li class="active">
                <strong>Staff</strong>
            </li>
        </ol>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>List of Staff</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>
                </div>
                <div class="ibox-content">

<table class="table table-striped">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.StaffName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff_Email)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Staff_ContactNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff_Qualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff_Technology)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff_Salary)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff_Designation)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in (List<MinusBugs.Institute.Web.FrontEnd.Models.Staff>)ViewBag.StaffList) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.StaffName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Staff_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Staff_Email)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.Staff_ContactNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Staff_Qualification)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Staff_Technology)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Staff_Salary)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Staff_Designation)
        </td>
      
        <td>
			@Html.ActionLink("Details", "Details", new { id=item.StaffId }, new { @class = "btn btn-primary btn-sm"})
            @Html.ActionLink("Edit", "Edit", new { id=item.StaffId }, new { @class = "btn btn-white btn-sm"})
            @Html.ActionLink("Delete", "Delete", new { id=item.StaffId }, new { @class = "btn btn-white btn-sm"})
        </td>
    </tr>
}

</table>

                </div>
            </div>
        </div>
    </div>
 </div>



